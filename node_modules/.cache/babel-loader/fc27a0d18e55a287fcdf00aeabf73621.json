        <Search @searchTrigger=\"searchCards\"></Search>\n      </header>\n      <!-- end of page header -->\n      <AddNewCard @addCardTrigger=\"addCard\"></AddNewCard>\n      <p class=\"likecounter\">You have liked {{ likes }} cards so far</p>\n      <ul class=\"cards-box\">\n        <Card\n          v-for=\"card in filteredData\"\n          :key=\"card.id\"\n          :card=\"card\"\n          @likeTrigger=\"recalculateLikes\"\n          @deleteTrigger=\"deleteCard\"\n        ></Card>\n      </ul>\n      <!-- end of cards box -->\n    </main>\n  </div>\n</template>\n\n<script>\nimport Search from './components/Search';\nimport AddNewCard from './components/AddNewCard';\nimport Card from './components/Card';\n\nconst uuidv4 = require('uuid/v4');\nconst colors = ['-orange', '-red', '-purple', '-blue', '-green'];\n\nlet cards = [\n  {\n    id: uuidv4(),\n    front: 'Who would I like to work with? ðŸ¤”',\n    back: '10Clouds! ðŸ˜Ž',\n    flipped: false,\n    liked: true,\n    color: `${colors[Math.floor(Math.random() * colors.length)]}`,\n  },\n  {\n    id: uuidv4(),\n    front:\n      'Job interviewer: \"And where would you see yourself in five yearsâ€™ time Mr. Jeffries?\" ðŸ¤­',\n    back:\n      'Mr. Jeffries: \"Personally I believe my biggest weakness is in listening.\" ðŸ¤£',\n    flipped: false,\n    liked: false,\n    color: `${colors[Math.floor(Math.random() * colors.length)]}`,\n  }\n];\n\nexport default {\n  name: 'app',\n  data() {\n    return {\n      cards: cards,\n      searchWord: '',\n      likes: cards.filter((card) => card.liked).length,\n    };\n  },\n  methods: {\n    searchCards(keyword) {\n      this.searchWord = keyword;\n    },\n    recalculateLikes(value) {\n      value ? this.likes++ : this.likes--;\n    },\n    deleteCard(id) {\n      this.cards = this.cards.filter((card) => {\n        if (card.id === id && card.liked) {\n          this.likes--;\n        }\n        return card.id !== id;\n      });\n    },\n    addCard(data) {\n      this.cards.unshift(data);\n    },\n  },\n  computed: {\n    filteredData() {\n      return this.cards.filter((card) => {\n        return card.front\n          .toLowerCase()\n          .includes(this.searchWord.trim().toLowerCase());\n      });\n    },\n  },\n  components: {\n    Search,\n    AddNewCard,\n    Card,\n  },\n};\n</script>\n\n<style lang=\"sass\">\n@import 'src/assets/sass/main.sass';\n\n.page-container\n  margin-left: auto\n  margin-right: auto\n  max-width: 1120px\n  padding-top: 1.5rem\n  padding-left: 1rem\n  padding-right: 1rem\n  overflow: hidden\n\n.top-message\n  font-size: 0.8571rem\n  text-align: left\n  color: $muted\n  .text\n    display: flex\n    align-items: center\n  .icon\n    width: 20px\n    fill: #ff4242\n    margin-right: 9px\n\n.page-header\n  padding-bottom: 40px\n  border-bottom: 1px dashed #dedede\n  .title\n    font-size: 2.286rem\n    font-weight: $bold\n    text-align: center\n    margin-top: 3.5rem\n    margin-bottom: 3rem\n\n.likecounter\n  font-size: 1.143rem\n  font-weight: $bold\n\n.cards-box\n  margin-top: 1rem\n  margin-left: -1.5rem\n  margin-right: -1.5rem\n  display: flex\n  flex-wrap: wrap\n  justify-content: center\n</style>\n"],"sourceRoot":"src"}]}